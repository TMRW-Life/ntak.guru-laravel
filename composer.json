{
    "name": "tmrw-life/ntak-guru-laravel",
    "description": "This is my package ntak-guru-laravel",
    "keywords": [
        "tmrw",
        "tmrw-life",
        "ntak",
        "viza",
        "laravel",
        "ntak-guru-laravel"
    ],
    "homepage": "https://github.com/TMRW-Life/ntak.guru-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Otta Levente",
            "email": "leventeotta@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.14",
        "illuminate/http": "^10.14",
        "illuminate/support": "^10.14"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "phpunit/phpunit": "^10.2",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "TmrwLife\\NtakGuru\\": "src/",
            "TmrwLife\\NtakGuru\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TmrwLife\\NtakGuru\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "TmrwLife\\NtakGuru\\NtakGuruServiceProvider"
            ],
            "aliases": {
                "NtakGuru": "TmrwLife\\NtakGuru\\Facades\\NtakGuru"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
